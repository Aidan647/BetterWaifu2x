{
	"env": {
		"node": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/eslint-recommended", "prettier"],
	"overrides": [],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"indent": [2, "tab"],
		"linebreak-style": [2, "unix"],
		"quotes": [1, "double"],
		"semi": [1, "never"]
	}
}
